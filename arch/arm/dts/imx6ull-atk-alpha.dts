/dts-v1/;

#include "imx6ull.dtsi"
#include "imx6ul-14x14-evk.dtsi"

/ {
	model = "Alientek Alpha Board";
	compatible = "fsl,imx6ull-atk-alpha", "fsl,imx6ull";
};

&clks {
	assigned-clocks = <&clks IMX6UL_CLK_PLL3_PFD2>;
	assigned-clock-rates = <320000000>;
};
&usdhc2{
		   /* drivers/mmc/fsl_esdhc_imx.c fsl_esdhc_ids */
		   compatible="fsl,esdhc";
		   bus-width=<8>;
		   non_removable;
		   status="okay";
		   /*
		 aliases {
			 mmc0 = &usdhc1;
			 mmc1 = &usdhc2;
		 };
		 */
};

